class Punto {
    constructor(x, y) {
        this.x =3D x;
        this.y =3D y;
    }
}

let modoDibujo =3D 'rasterizada'; // Modo predeterminado

function generarPuntos(n) {
    const puntos =3D [];
    for (let i =3D 0; i &lt; n; i++) {
        const x =3D Math.random() * 500;
        const y =3D Math.random() * 500;
        puntos.push(new Punto(x, y));
    }
    return puntos;
}

// Dibuja la figura en el canvas
function dibujarPuntos(ctx, puntos) {
    ctx.fillStyle =3D 'blue';
    puntos.forEach(p =3D&gt; {
        ctx.beginPath();
        ctx.arc(p.x, p.y, 5, 0, Math.PI * 2);
        ctx.fill();
    });
}

function dibujarFigura(ctx, puntos) {
    ctx.strokeStyle =3D 'red';
    ctx.beginPath();
    ctx.moveTo(puntos[0].x, puntos[0].y);
    puntos.forEach(p =3D&gt; {
        ctx.lineTo(p.x, p.y);
    });
    ctx.closePath();
   =20
    if (modoDibujo =3D=3D=3D 'rasterizada') {
        ctx.fillStyle =3D 'rgba(255, 0, 0, 0.3)';
        ctx.fill();
    }
    ctx.stroke();
}

// Dibujar en SVG
function dibujarSVG(puntos) {
    const svgContainer =3D document.getElementById('svgContainer');
    svgContainer.innerHTML =3D '';  // Limpiar cualquier contenido SVG prev=
io

    const svgNS =3D "http://www.w3.org/2000/svg";
    const svg =3D document.createElementNS(svgNS, "svg");
    svg.setAttribute("width", "500");
    svg.setAttribute("height", "500");

    const polygon =3D document.createElementNS(svgNS, "polygon");
    const pointsAttr =3D puntos.map(p =3D&gt; <code>${p.x},${p.y}</code>).j=
oin(" ");
    polygon.setAttribute("points", pointsAttr);
    polygon.setAttribute("fill", "rgba(255, 0, 0, 0.3)");
    polygon.setAttribute("stroke", "red");
   =20
    svg.appendChild(polygon);
    svgContainer.appendChild(svg);
}

function direccion(p1, p2, p3) {
    return (p2.x - p1.x) * (p3.y - p1.y) - (p2.y - p1.y) * (p3.x - p1.x);
}

function esConvexa(puntos) {
    let signo =3D null;
    for (let i =3D 0; i &lt; puntos.length; i++) {
        const p1 =3D puntos[i];
        const p2 =3D puntos[(i + 1) % puntos.length];
        const p3 =3D puntos[(i + 2) % puntos.length];
        const d =3D direccion(p1, p2, p3);

        if (d !=3D=3D 0) {
            const actualSigno =3D d &gt; 0;
            if (signo =3D=3D=3D null) {
                signo =3D actualSigno;
            } else if (signo !=3D=3D actualSigno) {
                return false; // Hay cambios de signo, es c=C3=B3ncava
            }
        }
    }
    return true; // No hay cambios de signo, es convexa
}

document.getElementById('btnRasterizada').addEventListener('click', () =3D&=
gt; {
    modoDibujo =3D 'rasterizada';
