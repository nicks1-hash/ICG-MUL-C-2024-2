document.getElementById('btnGenerar').addEventListener('click', () =3D&gt; =
{
    const cantidadPuntos =3D Math.floor(Math.random() * (20 - 3)) + 3; // E=
ntre 3 y 20 puntos
    const puntos =3D generarPuntos(cantidadPuntos);

