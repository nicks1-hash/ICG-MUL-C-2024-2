        const canvas =3D document.getElementById('canvas');
        const ctx =3D canvas.getContext('2d');
        ctx.clearRect(0, 0, canvas.width, canvas.height);
        dibujarPuntos(ctx, puntos);
        dibujarFigura(ctx, puntos);
    } else if (modoDibujo =3D=3D=3D 'vectorizada') {
        dibujarSVG(puntos);
    }

    const convexa =3D esConvexa(puntos);
    const tipo =3D convexa ? 'convexa' : 'c=C3=B3ncava';
    document.getElementById('info').innerText +=3D <code> | La figura gener=
ada es ${tipo}.</code>;
});

